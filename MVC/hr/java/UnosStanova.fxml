			</rowConstraints>
			<children>
				<Label text="Unos stanova" GridPane.columnSpan="2"
					GridPane.halignment="CENTER" />
				<Label text="Naslov:" GridPane.rowIndex="1" />
				<Label text="Opis:" GridPane.rowIndex="2" />
				<Label text="Kvadratura:" GridPane.rowIndex="3" />
				<Label text="Cijena:" GridPane.rowIndex="4" />
				<Button mnemonicParsing="false" onAction="#unosStanova"
					text="Unos" GridPane.columnSpan="2" GridPane.halignment="CENTER"
					GridPane.rowIndex="6" />
				<TextField fx:id="naslovTextField"
					GridPane.columnIndex="1" GridPane.rowIndex="1" />
				<TextField fx:id="opisTextField" GridPane.columnIndex="1"
					GridPane.rowIndex="2" />
				<TextField fx:id="kvadraturaTextField"
					GridPane.columnIndex="1" GridPane.rowIndex="3" />
				<TextField fx:id="cijenaTextField" prefHeight="25.0"
					prefWidth="193.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
				<Label text="Stanje:" GridPane.rowIndex="5" />
				<ComboBox fx:id="stanjeComboBox" prefWidth="150.0"
					GridPane.columnIndex="1" GridPane.rowIndex="5" />
			</children>
		</GridPane>
	</center>
